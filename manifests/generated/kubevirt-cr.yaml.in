---
apiVersion: kubevirt.io/v1
kind: KubeVirt
metadata:
  name: kubevirt
  namespace: {{.Namespace}}
spec:
  certificateRotateStrategy: {}
  configuration:
    developerConfiguration:
      featureGates:{{if .FeatureGates}}
      {{- range .FeatureGates}}
      - {{.}}
      {{- end}}{{else}} []{{end}}
  customizeComponents: {}
  imagePullPolicy: {{.ImagePullPolicy}}
  workloadUpdateStrategy: {}
